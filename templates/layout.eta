<!DOCTYPE html>
<html class="<%= it.type %>">
	<head>
		<meta charset="utf-8">
		<meta name="viewport" content="width=device-width, user-scalable=no, minimal-ui">
		<title><%= it.resume.basics?.name ? it.resume.basics.name : ''%></title>
		<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.1.1/css/all.min.css" integrity="sha512-KfkfwYDsLkIlwQp6LFnl8zNdLGxu9YAA1QvwINks4PhcElQSvqcyVLLD9aMhXd13uQjoXtEKNosOWaZqXgel0g==" crossorigin="anonymous" referrerpolicy="no-referrer" />
		
		<% if (it.type === 'PRINT') { %>
			<script src="https://cdn.jsdelivr.net/npm/jquery@3.6.0/dist/jquery.min.js"></script>
			<script src="https://cdn.jsdelivr.net/npm/@leisn/tocjs@0.1.0/dist/toc.min.js"></script>
			<script src="https://unpkg.com/pagedjs/dist/paged.polyfill.js"></script>
		<% } %>

		<style>
			<%~ it.mainCss %>
			<%~ it.widgetCss %>
		</style>
	</head>
	<body>
		<main>
		<div class="main-div">
		<div class="header">
			<!-- BASICS -->
			<%~ include("basic", {
				resume: it.resume
			}) %>
		</div>
		<div class="left--right--section--container">
		<div class="left-section">

			<!-- WORK -->
			<%~ include("work", {
				resume: it.resume
			}) %>

			<!-- PROJECT -->
			<%~ include("project", {
				resume: it.resume
			}) %>

			<!-- REFERENCE -->
			<%~ include("reference", {
				resume: it.resume
			}) %>

			<!-- AWARDS -->
			<%~ include("awards", {
				resume: it.resume
			}) %>
			
			<!-- EDUCATION -->
			<%~ include("education", {
				resume: it.resume
			}) %>
		 </div>
		 <div class="right-section">
		 
			<!-- PUBLICATION -->
			<%~ include("publication", {
				resume: it.resume
			}) %>
			
			<!-- VOLUNTEER -->
			<%~ include("volunteer", {
				resume: it.resume
			}) %>
			
			<!-- CERTIFICATION -->
			<%~ include("certification", {
				resume: it.resume
			}) %>

			<!-- SKILLS -->
			<%~ include("skills", {
				resume: it.resume
			}) %>
			
			<!-- INTEREST -->
			<%~ include("interest", {
				resume: it.resume
			}) %>

			<!-- LANGUAGE -->
			<%~ include("language", {
				resume: it.resume
			}) %>

			<!-- PROFILE -->
			<%~ include("profile", {
				resume: it.resume
			}) %>
		 </div>
		 </div>
			</div>
		</main>
	</body>
</html>
